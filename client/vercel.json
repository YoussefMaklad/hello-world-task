{
  "version": 2,
  "builds": [
    {
      "src": "../server/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "../server/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}